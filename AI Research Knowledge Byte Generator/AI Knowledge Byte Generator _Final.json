        "inputSchema": "{\n\t\"type\": \"object\",\n\t\"properties\": {\n\t\t\"title\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"description\": \"Catchy title for the knowledge byte\"\n\t\t},\n\t\t\"description\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"description\": \"Clear 2-3 sentence explanation of the topic.Focus on business value, impact, or risk reduction\"\n\t\t},\n      \"Howitworks\": {\n\t\t\t\"type\": \"array\",\n\t\t\t\"items\": {\n\t\t\t\t\"type\": \"string\"\n\t\t\t},\n\t\t\t\"description\": \"3‚Äì4 bullets about how it works,Describe the flow conceptually,Avoid deep technical jargon\"\n\t\t},\n\t\t\"interestingFacts\": {\n\t\t\t\"type\": \"array\",\n\t\t\t\"items\": {\n\t\t\t\t\"type\": \"string\"\n\t\t\t},\n\t\t\t\"description\": \"3-4 interesting facts about the topic\"\n\t\t},\n\t\t\"realUseCase\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"description\": \"Latest 2-3 real-world enterprise- relevant use case or application with specific example,Prefer internal, B2B, or platform use cases\"\n\t\t},\n      \"Key takeaway\": {\n\t\t\t\"type\": \"string\",\n\t\t\t\"description\": \"One short, memorable sentence\"\n\t\t}\n\t}\n}"
